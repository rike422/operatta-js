{
  "presets": [
    "es2015",
    "es2017",
    "stage-0",
    "babel-preset-power-assert"
  ],
  "plugins": [
    "babel-plugin-transform-flow-strip-types",
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "test": "./test"
      }
    }]
  ]
}
