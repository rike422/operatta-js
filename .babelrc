{
  "presets": [
    "es2015",
    "es2017",
    "stage-0"
  ],
  "plugins": [
    "babel-plugin-transform-flow-strip-types",
    "transform-decorators-legacy",
    "babel-plugin-espower",
    "transform-object-rest-spread"
  ],
  "env": {
    "AVA": {
      "plugins": [
        [
          "babel-plugin-webpack-loaders",
          {
            "config": "${CONFIG}",
            "verbose":true
          }
        ]
      ]
    }
  }
}